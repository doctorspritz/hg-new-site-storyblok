---
export interface Props {
  text?: string;
  href?: string;
  src?: string;
  srcset?: string;
  alt?: string;
  title?: string;
  value?: string;
  placeholder?: string;
  ariaLabel?: string;
}
const {
  text = '',
  href = '#',
  src = '',
  srcset = '',
  alt = '',
  title = '',
  value = '',
  placeholder = '',
  ariaLabel = '',
} = Astro.props;
---

<input
  maxlength="400"
  class="wpcf7-form-control wpcf7-tel wpcf7-text wpcf7-validates-as-required wpcf7-validates-as-tel"
  aria-required="true"
  aria-invalid="false"
  type="tel"
  name="phone-findaloan"
/>
