---
export interface Props {
  text?: string;
  href?: string;
  src?: string;
  srcset?: string;
  alt?: string;
  title?: string;
  value?: string;
  placeholder?: string;
  ariaLabel?: string;
}
const {
  text = '',
  href = '#',
  src = '',
  srcset = '',
  alt = '',
  title = '',
  value = '',
  placeholder = '',
  ariaLabel = '',
} = Astro.props;
---

<button class="btn_yellow header__menu-trigger">{{ text }}</button>
